echo "Enter the value of a"
read a
echo "enter the value of b"
read b
echo "Enter the value of c"
read c
echo "enter the value of d"
read d

if [[ $d -eq 0 ]]
then
        echo "division with 0 is not possible"
else
        val=`expr $a \* 20 - $b \* 2 + $c / $d `
        echo "the value after evaluation is $val"

fi

